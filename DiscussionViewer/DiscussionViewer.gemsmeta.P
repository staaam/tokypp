new_id(ID) :-
  findall(I,self_type(I,_),Ids),
  new_id(Ids,ID).

new_id(Ids,ID) :-
   max_element(1,Ids,Max),
   ID is Max + 1.

max_element(Curr,[],Curr).

max_element(Curr,[H|T],Max) :-
   Max2 is max(Curr,H),
   max_element(Max2,T,Max).

connect_all(List,Type, Result) :-
   new_id(ID),
   connect_all(List,Type,ID,Result).

connect_all([],_,_,[]).

connect_all([Src,Trg|Rest],Type, Id, Result) :-
   Id2 is Id +1,
   connect_all(Rest,Type,Id2,Result2),
   make_conn(Src,Trg,Type,Id,Result2,Result).

make_conn(Src,Trg,Type,Id,Curr, [[add,self_type,Id,Type],[add,self_source,Id,Src],[add,self_target,Id,Trg] | Curr]).

relate_all([],_,[]).

relate_all([Src,Trg|Rest],Relation, [[add,Relation,Src,Trg]|Result2]) :-
   relate_all(Rest,Relation,Result2).

