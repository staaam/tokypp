<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2005/GraphicalDefinition" name="editor">
  <figures
      name="Default">
    <figures
        xsi:type="gmfgraph:Rectangle"
        referencingElements="Opinion"
        name="OpinionFigure"/>
    <figures
        xsi:type="gmfgraph:Rectangle"
        referencingElements="Quote"
        name="QuoteFigure"/>
    <figures
        xsi:type="gmfgraph:PolylineConnection"
        referencingElements="Relation"
        name="RelationFigure"/>
  </figures>
  <nodes
      name="Opinion"
      figure="OpinionFigure"/>
  <nodes
      name="Quote"
      figure="QuoteFigure"/>
  <connections
      name="Relation"
      figure="RelationFigure"/>
</gmfgraph:Canvas>
